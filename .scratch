import java.time.Instant;
import java.time.ZoneOffset;
import java.time.ZonedDateTime;
import java.time.format.DateTimeFormatter;

public class AddSecondsToTime {
    public static void main(String[] args) {
        // Get current system time
        Instant now = Instant.now();

        // Add n seconds (e.g., 30 seconds)
        int secondsToAdd = 30;
        Instant futureTime = now.plusSeconds(secondsToAdd);

        // Format the time in UTC
        DateTimeFormatter formatter = DateTimeFormatter.ofPattern("yyyy-MM-dd HH:mm:ss")
                                                       .withZone(ZoneOffset.UTC);

        // Print the formatted time
        String formattedTime = formatter.format(futureTime);
        System.out.println("Time after " + secondsToAdd + " seconds in UTC: " + formattedTime);
    }
}
